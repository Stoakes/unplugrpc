{
  "name": "unplugrpc-gateway",
  "version": "0.1.0",
  "description": "Expose your gRPC endpoints through a HTTP API",
  "main": "index.js",
  "bin": {
    "unplugrpc-gateway": "index.js"
  },
  "scripts": {
    "start": "node ./dist/server.js",
    "dev": "node ./dist/server.js --cors",
    "build": "npm run build:ui && npm run build:gateway && echo \"Done.\"",
    "build:ui": "mkdir -p build-ui; cd ../ui; npm install --production && npm run build; cp -R build/* ../gateway/build-ui; cd ../gateway",
    "build:gateway": "tsc && npm run lint",
    "test": "jest --forceExit --coverage --verbose",
    "watch": "tsc -w",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "lint-fix": "tslint -c tslint.json -p tsconfig.json --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stoakes/unplugrpc.git"
  },
  "keywords": [],
  "author": "Antoine Beyet <Stoakes@users.noreply.github.com>",
  "license": "AGPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/stoakes/unplugrpc/issues"
  },
  "homepage": "https://github.com/stoakes/unplugrpc#readme",
  "dependencies": {
    "body-parser": "^1.18.2",
    "chalk": "^2.3.2",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "express-validator": "^5.3.0",
    "grpc": "^1.10.1",
    "lowdb": "^1.0.0",
    "protocol-buffers-schema": "^3.3.2",
    "yargs": "^12.0.1"
  },
  "devDependencies": {
    "@types/body-parser": "^1.16.8",
    "@types/cors": "^2.8.4",
    "@types/express": "^4.16.0",
    "@types/lowdb": "^1.0.4",
    "@types/node": "^10.5.4",
    "@types/yargs": "^11.1.1",
    "jest": "^22.0.4",
    "supertest": "^3.0.0",
    "ts-jest": "^22.0.4",
    "ts-node": "^5.0.0",
    "tslint": "^5.9.1",
    "typescript": "^2.7.2"
  }
}
